for(let i=1;i<=5;i++){
    let str=""
    for(let j=1;j<=6-i;j++){
        str=str+"*"
    }
    for(let j=1;j<=2*i-2;j++){
        str=str+" "
    }
    for(let j=1;j<=6-i;j++){
        str=str+"*"
    }
  console.log(str);
   
}
for(let i=5;i>=1;i--){
    let str=""
    for(let j=1;j<=6-i;j++){
        str=str+"*"
    }
    for(let j=1;j<=2*i-2;j++){
        str=str+" "
    }
    for(let j=1;j<=6-i;j++){
        str=str+"*"
    }
    console.log(str);
}
